	/* GIT */
	p, pre {margin-top: 2px; color: #555;} 
	git-title {padding-bottom: 10px; border-bottom: 1px solid #eaecef; margin-bottom: 10px;}
	/*无序圆点列表*/
	.ul-point {padding: 0px 20px;}
	.ul-point, .ul-point > li {margin-top: 2px;}
	/*有序*/
	.ul-ol {margin: 0 4px;}
	.ul-ol, .ul-ol > li {margin-top: 2px;}
	/*高亮*/
	.p-box {display: initial; border-radius: 4px;  background: gray; color: #fff; padding: 3px 5px;}
	/*加粗*/
	.font-strong {font-weight: bold;}
	/*分隔线*/
	.p-sep-line-1 {border-top: 1px solid #e6e6e6;}
	.p-sep-line-2 {border: 1px solid #e6e6e6;}
	.p-sep-line-3 {border: 2px solid #e6e6e6;}
	.p-sep-line-4 {border: 3px solid #e6e6e6;}
	/*链接*/
	.a-url {text-decoration: none; cursor: pointer;}
	.a-url:visited {color:deepskyblue;}
	.a-url:hover {color: red;}
	.a-url:active {color: red;}
	/*行样式*/
	.pre-line {min-height:16px; white-space: pre-wrap; word-wrap: break-word;}

	/*table 表格样式*/
	.commonly-table {display: block; overflow: hidden; width: 100%; border-collapse: collapse; border-spacing: 0; margin: 10px 0px;}
	.commonly-table tr {background: #fff;}
	.commonly-table th {font-weight: 500; border: 1px solid #e6e6e6;}
	.commonly-table th, .commonly-table td {padding: 6px 13px; border-left: 1px solid #e6e6e6; border-right: 1px solid #e6e6e6;}
	.commonly-table tr:last-child {border-bottom: 1px solid #e6e6e6}

	.commonly-table tr{height:35px;}
	.commonly-table .tr-odd{background:#f6f4f0;}