  <link type="text/css" rel="stylesheet" href="../css/MyCss.css" />
  <link type="text/css" rel="stylesheet" href="../css/git-plugin.css" />
  <style>
	*{margin: 0; padding: 0; font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Helvetica,Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol";}
	body {background: #e4e3e3;}
	
	.body {width: 1490px; margin: 10px auto; padding: 10px; background: #fff; border-radius: 4px;}

	.div-textarea {float: left; padding: 30px 0;}
	.div-textarea textarea {border: 1px solid #d1d5da; border-radius: 4px; padding: 10px;  outline: none; font-family: "SFMono-Regular",Consolas,"Liberation Mono",Menlo,Courier,monospace; color: #555;}
	.div-textarea input {float: right;}

	.git-cnt {width: 667px; height: 768px; padding: 36px; font-size: 14px; float: left; border: 1px solid #d1d5da; border-radius: 4px; margin-top: 29px; margin-left: 10px; overflow-y: auto;}

	/* GIT */
	p, pre {margin-top: 2px; color: #555;} 
	h1, h2, h3, h4, h4, h5, h6 {padding-bottom: 10px; border-bottom: 1px solid #eaecef; margin-bottom: 10px; margin-top: 10px;}
	/*无序圆点列表*/
	.ul-point {padding: 0px 20px;}
	.ul-point, .ul-point > li {margin-top: 2px;}
	/*有序*/
	.ul-ol {margin: 0 4px;}
	.ul-ol, .ul-ol > li {margin-top: 2px;}
	/*高亮*/
	.p-box {display: initial; border-radius: 4px;  background: #ece8dd; color: #555; padding: 3px 5px;}
	/*加粗*/
	.font-strong {font-weight: bold;}
	/*分隔线*/
	.p-sep-line-1 {border-top: 1px solid #e6e6e6;}
	.p-sep-line-2 {border: 1px solid #e6e6e6;}
	.p-sep-line-3 {border: 2px solid #e6e6e6;}
	.p-sep-line-4 {border: 3px solid #e6e6e6;}
	/*链接*/
	.a-url {text-decoration: none; cursor: pointer;}
	.a-url:visited {color:deepskyblue;}
	.a-url:hover {color: red;}
	.a-url:active {color: red;}
	/*行样式*/
	.pre-line {min-height:16px; white-space: pre-wrap; word-wrap: break-word;}

	/*table 表格样式*/
	.git-cnt table {display: block; overflow: hidden; width: 100%; border-collapse: collapse; border-spacing: 0; margin: 10px 0px;}
	.git-cnt table tr {background: #fff;}
	.git-cnt table th {font-weight: 500; border: 1px solid #e6e6e6;}
	.git-cnt table th, .git-cnt table td {padding: 6px 13px; border-left: 1px solid #e6e6e6; border-right: 1px solid #e6e6e6;}
	.git-cnt table tr:last-child {border-bottom: 1px solid #e6e6e6}

	.git-cnt table tr{height:35px;}
	.git-cnt table .tr-odd{background:#f6f4f0;}
  </style>
 </head>
