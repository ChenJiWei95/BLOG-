	function initAndUpdate(){
		//对导航条的初始化和随着窗口的变动进行更新
		var bodyNavNo = $(".body-nav-no")[0];
		var marginLeft = bodyNavNo.getBoundingClientRect().left;
		var width = bodyNavNo.clientWidth;
		var bodyNav = $(".body-nav")[0];
		
